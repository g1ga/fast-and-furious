<Window x:Class="MvvmCommands.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label Content="{Binding Counter}" HorizontalAlignment="Left" Height="29" Margin="69,45,0,0" VerticalAlignment="Top" Width="101" Background="#FFAEEE52" FontSize="16"/>
        <Button Command="{Binding IncrementCommand}" Content="Increment" HorizontalAlignment="Left" Height="30" Margin="69,79,0,0" VerticalAlignment="Top" Width="101"/>
        <Button Command="{Binding DecrementCommand}" Content="Decrement" HorizontalAlignment="Left" Height="30" Margin="69,114,0,0" VerticalAlignment="Top" Width="101"/>
    </Grid>
</Window>
